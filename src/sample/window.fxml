<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="250.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="250.0" prefWidth="300.0" VBox.vgrow="ALWAYS">
      <children>
            <TextArea fx:id="receiveText" layoutX="167.0" layoutY="27.0" prefHeight="168.0" prefWidth="172.0" />
            <TextField fx:id="sendText" layoutX="28.0" layoutY="27.0" prefHeight="25.0" prefWidth="118.0" />
            <Button fx:id="sendButton" onAction="#buttonClicked" layoutX="49.0" layoutY="60.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="Send" />
      </children>
    </AnchorPane>
  </children>
</VBox>
